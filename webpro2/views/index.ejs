  <div>
    <center>

      <form action="/" method="POST" name="login" onsubmit="return  validateLogin()">
        <table border="5">

          <tr>
            <td>用户</td>
            <td><input type="text" name="user" onblur="use()"><text class="text"></text></td>

          </tr>
          <tr>
            <td>密码</td>
            <td><input type="password" name="pass" onblur="ps()"><text class="text"></text></td>
          </tr>

          <tr>
            <td><input type="submit" name="button" value="登录"><text id="text"></text></td>
            <td><text class="text">若未注册请先注册<a href="/users"><input type="button" name="button" value="注册"></a></text>
            </td>

          </tr>
        </table>
      </form>
